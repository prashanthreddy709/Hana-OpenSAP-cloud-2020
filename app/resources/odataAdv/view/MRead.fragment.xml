<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Panel id="panel" expandable="true" expanded="true" headerText="{i18n>P1}">
		<List id="list" width="100%">
			<InputListItem id="itmPath" label="{i18n>L1}">
				<Input id="mPath" value="{/mPath}"/>
			</InputListItem>
			<InputListItem id="itmEntity1" label="{i18n>L2}">
				<Input id="mEntity1" value="{/mEntity1}"/>
			</InputListItem>
			<InputListItem id="itmEntity2" label="{i18n>L3}">
				<Input id="mEntity2" value="{/mEntity2}"/>
			</InputListItem>
		</List>
		<Button id="btnCall" press="callMultiService" text="{i18n>B1}"/>
		<Button id="btnExcel" press="callExcel" text="{i18n>B2}"/>
	</Panel>
	<core:Fragment id="frg1" fragmentName="sap.openSAP.odataAdv.view.MTableHead" type="XML"/>
	<core:Fragment id="frg2" fragmentName="sap.openSAP.odataAdv.view.MTableItem" type="XML"/>
</core:FragmentDefinition>